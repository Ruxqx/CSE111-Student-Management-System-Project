// https://dbdiagram.io/d
Table course{
    c_id integer [primary key]
    dept_id integer
    name string
    desc string
}

Table section{
    section_id integer [primary key]
    c_id integer
    year integer
    semester string
}

Table instructorSection{
    section_id integer [primary key]
    i_id integer [primary key]
    role_in_section string
}

Table instructor{
    i_id integer [primary key]
    dept_id integer
    name string
    office string
    email string
    role string

}

Table enrollment{
    enroll_id integer [primary key]
    student_id integer
    section_id integer
    enroll_date string
    grade string
}

Table student{
    student_id integer [primary key]
    name string
    major string
    enroll_year string
    email string
}

Table department{
    dept_id integer [primary key]
    name string
    building string
    chair_id integer
}

Table officeHours{
    oh_id integer [primary key]
    i_id integer
    day string
    start_time string
    end_time string
    location string
}

Table user{
  user_id integer [primary key]
  username string
  password string
  linked_id integer
}

Ref linked:user.linked_id-student.student_id
Ref linked:user.linked_id-instructor.i_id


Ref offers:course.dept_id>department.dept_id
Ref has:instructor.dept_id>department.dept_id
Ref has:instructor.i_id-department.chair_id


Ref offered_in:section.c_id>course.c_id

Ref belongs : section.section_id < instructorSection.section_id
Ref belongs : instructor.i_id < instructorSection.i_id
Ref has : instructor.i_id < officeHours.i_id


Ref enrolls : enrollment.student_id > student.student_id
Ref in : enrollment.section_id > section.section_id





